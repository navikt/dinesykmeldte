{
  "name": "dinesykmeldte",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run start",
    "start": "concurrently -n next,codegen \"npm run start:watch\" \"npm run gen:watch\"",
    "start:watch": "next dev | pino-pretty",
    "build": "next build",
    "test:watch": "vitest --config vitest.config.mts",
    "test": "vitest run --config vitest.config.mts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "gen": "graphql-codegen",
    "gen:watch": "graphql-codegen -w",
    "report-debt": "scripts/debt.sh",
    "preinstall": "husky"
  },
